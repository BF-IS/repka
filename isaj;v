#ex 2

x = 7
y = 3
z = 0
print(x + y, x - y, x * y, x / y, x // y, x ** y )
z = x ** 2 - (x * y)
print(z)

#ex 3

m = 8
n = 4
m = 8 + 4
n = m * 2
m = n % m
n = n - m
print(m, n)

#ex 4

a = 12
b = 8
c = 12
print(a > b, b < a, a == c, b == c, a >= c, b <= c)

#ex 5

